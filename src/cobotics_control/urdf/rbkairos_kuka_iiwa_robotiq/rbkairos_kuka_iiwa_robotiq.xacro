            <origin rpy="0 0 0" xyz="0 0.0006 0.0004" />
            <mass value="0.18" />
            <inertia ixx="0.0005" ixy="0" ixz="0" iyy="0.00036" iyz="0" izz="0.00047" />
        </inertial>

