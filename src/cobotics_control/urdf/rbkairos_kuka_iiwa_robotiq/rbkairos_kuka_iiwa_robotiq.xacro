<?xml version="1.0"?>
<robot name="rbkairos_kuka_iiwa_robotiq" xmlns:xacro="http://wiki.ros.org/xacro">
    <!-- Import rbkairos base -->
    <!-- <xacro:include filename="$(find rbkairos_description)/robots/rbkairos_base.urdf.xacro" />
    <xacro:rbkairos/> -->

    <!-- First we import all posible elements defined in the urdf.xacro files. All these elements are defined as macro:xacros -->
	<xacro:arg name="prefix" default="robot"/>
	<xacro:arg name="ros_planar_move_plugin" default="true"/>


    <!-- Import summit XL base elements -->
    <xacro:include filename="$(find rbkairos_description)/urdf/bases/rbkairos_base.urdf.xacro" />

    <xacro:rbkairos_base prefix="$(arg prefix)" publish_bf="true" hq="true"/>

   <xacro:if value="$(arg ros_planar_move_plugin)">
       <xacro:omni_steering prefix="$(arg prefix)" publish_tf="true"/>
   </xacro:if>

    <xacro:arg name="world_link" default="0"/>
    <xacro:if value="$(arg world_link)">
        <xacro:property name="base_link_name" value="world"/>
    </xacro:if>
    <xacro:unless value="$(arg world_link)">
        <xacro:property name="base_link_name" value="iiwa_base_link"/>
    </xacro:unless>

    <xacro:arg name="robot_ns" default="iiwa"/>
    <xacro:if value="${'$(arg robot_ns)' != '!'}">
        <xacro:property name="robot_namespace" value="$(arg robot_ns)::"/>
    </xacro:if>
    <xacro:unless value="${'$(arg robot_ns)' != '!'}">
        <xacro:property name="robot_namespace" value=""/>
    </xacro:unless>

    <xacro:property name="joint_damping" value="0.5"/>
    <xacro:property name="joint_friction" value="0.0"/>
    <xacro:property name="joint_spring_ref" value="0.0"/>
    <xacro:property name="joint_spring_stiffness" value="0.0"/>
    <xacro:property name="joint_stop_dissipation" value="0.0"/>

    <material name="Black">
        <color rgba="0.0 0.0 0.0 1.0" />
    </material>
    <material name="Blue">
        <color rgba="0.0 0.0 0.8 1.0" />
    </material>
    <material name="Green">
        <color rgba="0.0 0.8 0.0 1.0" />
    </material>
    <material name="Grey">
        <color rgba="0.4 0.4 0.4 1.0" />
    </material>
    <material name="Orange">
        <color rgba="1.0 0.423529411765 0.0392156862745 1.0" />
    </material>
    <material name="Brown">
        <color rgba="0.870588235294 0.811764705882 0.764705882353 1.0" />
    </material>
    <material name="Red">
        <color rgba="0.8 0.0 0.0 1.0" />
    </material>
    <material name="White">
        <color rgba="1.0 1.0 1.0 1.0" />
    </material>
    <!-- World. Affix this on spawn -->
    <!-- <link name="${robot_namespace}${base_link_name}" /> -->
    <!--joint between base and link_0-->
    <joint name="base_iiwa_joint" type="revolute">
        <origin rpy="0 0 0" xyz="0 0 0.38" />
        <parent link="$(arg prefix)base_link" />
        <child link="${robot_namespace}iiwa_link_0" />
        <limit lower="0" upper="0" effort="0" velocity="0" />
    </joint>
    <link name="${robot_namespace}iiwa_link_0">
        
        <inertial>
            <origin rpy="0 0 0" xyz="-0.1 0 0.07" />
            <mass value="5" />
            <inertia ixx="0.05" ixy="0" ixz="0" iyy="0.06" iyz="0" izz="0.03" />
        </inertial>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://cobotics_kuka_iiwa/meshes/visual/link_0.stl" />
            </geometry>
            <material name="Grey" />
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://cobotics_kuka_iiwa/meshes/collision/link_0.stl" />
            </geometry>
            <material name="Grey" />
        </collision>
        <self_collision_checking>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <capsule length="0.25" radius="0.15" />
            </geometry>
        </self_collision_checking>
    </link>
    <!-- joint between link_0 and link_1 -->
    <joint name="iiwa_joint_1" type="revolute">
        <parent link="${robot_namespace}iiwa_link_0" />
        <child link="${robot_namespace}iiwa_link_1" />
        <origin rpy="0 0 0" xyz="0 0 0.1575" />
        <axis xyz="0 0 1" />
        <limit effort="300" lower="-2.96705972839" upper="2.96705972839" velocity="10" />
        <safety_controller k_position="100" k_velocity="2" soft_lower_limit="-2.93215314335" soft_upper_limit="2.93215314335" />
        <dynamics damping="${joint_damping}" friction="${joint_friction}" />
    </joint>
    <link name="${robot_namespace}iiwa_link_1">
        
        <inertial>
            <origin rpy="0 0 0" xyz="0 -0.03 0.12" />
            <mass value="4" />
            <inertia ixx="0.1" ixy="0" ixz="0" iyy="0.09" iyz="0" izz="0.02" />
        </inertial>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://cobotics_kuka_iiwa/meshes/visual/link_1.stl" />
            </geometry>
            <material name="Orange" />
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://cobotics_kuka_iiwa/meshes/collision/link_1.stl" />
            </geometry>
            <material name="Orange" />
        </collision>
    </link>
    <!-- joint between link_1 and link_2 -->
    <joint name="iiwa_joint_2" type="revolute">
        <parent link="${robot_namespace}iiwa_link_1" />
        <child link="${robot_namespace}iiwa_link_2" />
        <origin rpy="1.57079632679   0 3.14159265359" xyz="0 0 0.2025" />
        <axis xyz="0 0 1" />
        <limit effort="300" lower="-2.09439510239" upper="2.09439510239" velocity="10" />
        <safety_controller k_position="100" k_velocity="2" soft_lower_limit="-2.05948851735" soft_upper_limit="2.05948851735" />
        <dynamics damping="${joint_damping}" friction="${joint_friction}" />
    </joint>
    <link name="${robot_namespace}iiwa_link_2">
        
        <inertial>
            <origin rpy="0 0 0" xyz="0.0003 0.059 0.042" />
            <mass value="0.4" />
            <inertia ixx="0.005" ixy="0" ixz="0" iyy="0.0018" iyz="0" izz="0.0044" />
        </inertial>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://cobotics_kuka_iiwa/meshes/visual/link_2.stl" />
            </geometry>
            <material name="Orange" />
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://cobotics_kuka_iiwa/meshes/collision/link_2.stl" />
            </geometry>
            <material name="Orange" />
        </collision>
    </link>
    <!-- joint between link_2 and link_3 -->
