            <mass value="0.3" />
            <inertia ixx="0.008" ixy="0" ixz="0" iyy="0.0075" iyz="0" izz="0.001" />
        </inertial>

        <visual>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://cobotics_kuka_iiwa/meshes/visual/link_3.stl" />
            </geometry>
            <material name="Orange" />
        </visual>
        <collision>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <geometry>
                <mesh filename="package://cobotics_kuka_iiwa/meshes/collision/link_3.stl" />
            </geometry>
            <material name="Orange" />
        </collision>
    </link>
    <!-- joint between link_3 and link_4 -->
