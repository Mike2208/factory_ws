    </gazebo>
    <gazebo reference="iiwa_joint_5">
        <dynamics>
            <spring_reference>${joint_spring_ref}</spring_reference>
            <spring_stiffness>${joint_spring_stiffness}</spring_stiffness>
        </dynamics>
    </gazebo>
    <gazebo reference="iiwa_joint_6">
        <dynamics>
            <spring_reference>${joint_spring_ref}</spring_reference>
            <spring_stiffness>${joint_spring_stiffness}</spring_stiffness>
        </dynamics>
    </gazebo>
    <gazebo reference="iiwa_joint_7">
        <dynamics>
            <spring_reference>${joint_spring_ref}</spring_reference>
            <spring_stiffness>${joint_spring_stiffness}</spring_stiffness>
        </dynamics>
        <!-- Enable the Joint Feedback -->
        <provideFeedback>true</provideFeedback>
    </gazebo>
    
    <!-- The ft_sensor plugin -->
    <gazebo>
        <plugin filename="libgazebo_ros_ft_sensor.so" name="ft_sensor">
            <updateRate>50.0</updateRate>
            <topicName>/iiwa/state/CartesianWrench</topicName>
            <jointName>iiwa_joint_7</jointName>
        </plugin>
        <plugin name="gazebo_grasp_fix" filename="libgazebo_grasp_fix.so">
            <arm>
                <arm_name>iiwa</arm_name>
                <palm_link>${robot_namespace}iiwa_link_7</palm_link>
                <gripper_link>${robot_namespace}gripper_right_link</gripper_link>
                <gripper_link>${robot_namespace}gripper_left_link</gripper_link>
            </arm>
            <forces_angle_tolerance>100</forces_angle_tolerance>
            <update_rate>4</update_rate>
            <grip_count_threshold>4</grip_count_threshold>
            <max_grip_count>8</max_grip_count>
            <release_tolerance>0.005</release_tolerance>
            <disable_collisions_on_attach>false</disable_collisions_on_attach>
            <contact_topic>__default_topic__</contact_topic>
        </plugin>
    </gazebo>
    <!-- Load Gazebo lib and set the robot namespace -->
    <gazebo>
        <plugin filename="libgazebo_ros_control.so" name="gazebo_ros_control">
	        <robotNamespace>/$(arg robot_ns)</robotNamespace>
            <robotSimType>IIWARobotHWSim</robotSimType>
        </plugin>
    </gazebo>
    <!-- Link0 -->
    <gazebo reference="${robot_namespace}iiwa_link_0">
        <material>Gazebo/Grey</material>
        <mu1>0.2</mu1>
        <mu2>0.2</mu2>
    </gazebo>
    <!-- Link1 -->
